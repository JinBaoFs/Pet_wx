.tabbar {
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: linear-gradient(180deg, #FFD111 0%, #FEE126 47.46%);
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20px;
  display: flex;
  border-top: 1px solid #eee;
  z-index: 99999;
}
.tabbar .tabbar-item {
  flex: 1;
  text-align: center;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-top: 0;
}
.tabbar .tabbar-item text {
  color: #000;
  font-size: 24rpx;
  margin-top: 0rpx;
}
.tabbar .tabbar-item .image-box {
  display: flex;
  justify-content: center;
  align-items: center;
  transition: all 0.5s;
}
.tabbar .tabbar-item-active text {
  font-weight: 600;
}
.tabbar .tabbar-item-active .image-box {
  width: 78rpx;
  height: 78rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
  background-position: center;
}
.tabbar .tabbar-item-container {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, 0);
  display: flex;
  flex-direction: column;
  gap: 8rpx;
  padding-bottom: 4rpx;
}
.tabbar .img-center-box {
  width: 140rpx;
  height: 140rpx;
  background: #fff;
  border-radius: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabbar .img-center-box image {
  width: 120rpx;
}
.tabbar .icon {
  width: 40rpx;
}